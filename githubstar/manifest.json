{
	"name": "Github Star",
	"version": "0.3.1.1",
	"manifest_version": 2,
	
	"description": "This extension displays the information(star, fork) from github project URL",
	"icons": { "128": "icon.png" },
	
	"content_scripts": [{
		"matches": ["<all_urls>"],
		"js": [
			"lib/jquery-1.9.1.min.js",
			"lib/lscache.js",
			"contents_script.js"
		]
	}],

	"options_page": "options.html",
	"permissions": [
		"https://api.github.com/",
		"storage"
	] 
}
